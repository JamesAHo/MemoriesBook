<div class="row">
  <div class="col-sm-8 col-sm-offset-2">
    <ul>
      {{#each posts}}
        <li class="list-group-item">
          <div class="lead">Title:{{this.title}}</div>
          <a href="{{this.summary}}" target="_blank">{{this.summary}}</a>
        </li>
      {{/each}}
    </ul>
  </div>
</div>